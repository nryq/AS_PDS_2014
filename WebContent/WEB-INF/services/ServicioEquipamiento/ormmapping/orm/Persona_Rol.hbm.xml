<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="orm.Persona_Rol" table="Persona_Rol" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<many-to-one name="id_Persona" cascade="lock" column="id_Persona" class="orm.Persona" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<many-to-one name="id_Rol" cascade="lock" column="id_Rol" class="orm.Rol" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
